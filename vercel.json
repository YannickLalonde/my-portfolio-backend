{
  "version": 2,
  "builds": [
    {
      "src": "portfolio_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "portfolio_backend/wsgi.py"
    }
  ],
  "functions": {
    "portfolio_backend/wsgi.py": {
      "memory": 1024,
      "maxDuration": 10,
      "regions": ["sin1"]
    }
  }
}